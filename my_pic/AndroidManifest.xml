        <!-- umeng -->
        <meta-data
            android:name="UMENG_APPKEY"
            android:value="53a8e9ed56240b03f000f7f3" >
        </meta-data>
        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="001" >
